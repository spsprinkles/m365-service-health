{
  "name": "m365-service-health",
  "version": "0.0.8",
  "description": "The M365 Service Health app is a solution that reads service health data from a SharePoint list and presents it to all users with an intuitive interface.",
  "main": "src/index.ts",
  "scripts": {
    "all": "npm run clean && npm run build && npm run prod && npm run flow && npm run spfx",
    "build": "webpack --mode development",
    "clean": "node ./clean.js",
    "flow": "node ./flow.js",
    "prod": "webpack --mode production",
    "spfx": "cd spfx && npm run package",
    "test": "npm run build && cd spfx && gulp serve"
  },
